{
  "name": "cost-of-cloud",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir dist",
    "build": "npm run babel",
    "deploy": "npm run build && npm run package && npm run launch",
    "eslint": "eslint src",
    "package": "sam package --template-file template.yml --s3-bucket cost-of-cloud --output-template-file packaged.yml",
    "launch": "sam deploy --template-file ./packaged.yml --stack-name cost-of-cloud --capabilities CAPABILITY_IAM",
    "invoke": "npm run build && sam local invoke --event event.json",
    "test": "npm run eslint",
    "execute": "babel-node src/index.js",
    "watch": "nodemon --exec npm run execute"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "aws-sam-local": "^0.2.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.10.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.4"
  },
  "dependencies": {
    "aws-sdk": "^2.142.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "p-settle": "^2.0.0",
    "superagent": "^3.8.0"
  },
  "pre-commit": [
    "eslint"
  ]
}
